@model List<Filme>@{ ViewData["Title"] = "Home Page";}<!-- Página de exibição da tabela preenchida com valores --><div class="text-center">    <h1>Lista de Filmes</h1>    <table class="table">        <thead>            <tr>                <th>@Html.DisplayNameFor(Model => Model.First().Id)</th>                <th>@Html.DisplayNameFor(Model => Model.First().Titulo)</th>                <th>@Html.DisplayNameFor(Model => Model.First().DataLancamento)</th>                <th>@Html.DisplayNameFor(Model => Model.First().Duracao)</th>                <th>Ações</th>            </tr>        </thead>        <tbody>            @foreach (Filme f in Model)            {            <tr>                <td>@Html.DisplayFor(modelItem => f.Id)</td>                <td>@Html.DisplayFor(modelItem => f.Titulo)</td>                <td>@Html.DisplayFor(modelItem => f.DataLancamento)</td>                <td>@Html.DisplayFor(modelItem => f.Duracao)</td>                <td>                    <a asp-action="Visualizar" asp-route-id="@f.Id">Visualizar</a>                    <a asp-action="Atualizar" asp-route-id="@f.Id">Atualizar</a>                    <a asp-action="ConfirmaDelete" asp-route-id="@f.Id">Deletar</a>                    <a asp-action="Sortear">Visualizar Ordenado</a>                </td>            </tr>}        </tbody>    </table>    <div>        <a asp-action="Create" class="btn btn-info">Cadastrar novo filme!</a>    </div></div>