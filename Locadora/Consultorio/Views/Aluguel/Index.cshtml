@model List<Aluguel>@{ ViewData["Title"] = "Aluguel";}<!-- Página de exibição da tabela preenchida com valores --><div class="text-center">    <h1>Lista de Filmes</h1>    <table class="table">        <thead>            <tr>                <th>@Html.DisplayNameFor(Model => Model.First().id)</th>                <th>@Html.DisplayNameFor(Model => Model.First().dataHora)</th>                <th>@Html.DisplayNameFor(Model => Model.First().devolucao)</th>                <th>@Html.DisplayNameFor(Model => Model.First().filme.Titulo)</th>                <th>Ações</th>            </tr>        </thead>        <tbody>            @foreach (var f in Model)            {            <tr>                <td>@Html.DisplayFor(modelItem => f.id)</td>                <td>@Html.DisplayFor(modelItem => f.dataHora)</td>                <td>@Html.DisplayFor(modelItem => f.devolucao)</td>                <td>@Html.DisplayFor(modelItem => f.filme.Titulo)</td>                <td>                    <a asp-action="Visualizar" asp-route-id="@f.id">Assistir</a>                    <a asp-action="Atualizar" asp-route-id="@f.id">Atualizar</a>                    <a asp-action="ConfirmaDelete" asp-route-id="@f.id">Deletar</a>                </td>            </tr>            }        </tbody>    </table>    <div>        <a asp-action="Create" class="btn btn-info">Cadastrar novo aluguel!</a>    </div></div>